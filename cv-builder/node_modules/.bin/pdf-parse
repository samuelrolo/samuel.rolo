#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/samuel-rolo-repo/cv-builder/node_modules/.pnpm/pdf-parse@2.4.5/node_modules/pdf-parse/bin/node_modules:/home/ubuntu/samuel-rolo-repo/cv-builder/node_modules/.pnpm/pdf-parse@2.4.5/node_modules/pdf-parse/node_modules:/home/ubuntu/samuel-rolo-repo/cv-builder/node_modules/.pnpm/pdf-parse@2.4.5/node_modules:/home/ubuntu/samuel-rolo-repo/cv-builder/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/samuel-rolo-repo/cv-builder/node_modules/.pnpm/pdf-parse@2.4.5/node_modules/pdf-parse/bin/node_modules:/home/ubuntu/samuel-rolo-repo/cv-builder/node_modules/.pnpm/pdf-parse@2.4.5/node_modules/pdf-parse/node_modules:/home/ubuntu/samuel-rolo-repo/cv-builder/node_modules/.pnpm/pdf-parse@2.4.5/node_modules:/home/ubuntu/samuel-rolo-repo/cv-builder/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/pdf-parse@2.4.5/node_modules/pdf-parse/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/pdf-parse@2.4.5/node_modules/pdf-parse/bin/cli.mjs" "$@"
fi
