{
  "version": 2,
  "name": "share2inspire",
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/cv-builder",
      "destination": "https://cv-builder.share2inspire.pt",
      "permanent": true
